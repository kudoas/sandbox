{
  "scopeName": "erb.javascript-tag.injection",
  "injectionSelector": "text.html.erb -comment -string, text.html.ruby -comment -string, text.html.erb.ruby -comment -string",
  "patterns": [
    {
      "name": "meta.embedded.block.javascript.erb",
      "begin": "(<%=)\\s*(javascript_tag)(?:\\([^%]*?\\))?\\s*(do)\\s*(%>)",
      "beginCaptures": {
        "1": { "name": "punctuation.section.embedded.begin.erb" },
        "2": { "name": "support.function.erb" },
        "3": { "name": "keyword.control.ruby" },
        "4": { "name": "punctuation.section.embedded.end.erb" }
      },
      "end": "(<%)(\\s*end\\s*)(%>)",
      "endCaptures": {
        "1": { "name": "punctuation.section.embedded.begin.erb" },
        "2": { "name": "keyword.control.ruby" },
        "3": { "name": "punctuation.section.embedded.end.erb" }
      },
      "contentName": "source.js",
      "patterns": [
        { "include": "source.js" }
      ]
    }
  ],
  "embeddedLanguages": {
    "source.js": "javascript"
  }
}
